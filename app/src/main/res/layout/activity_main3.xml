<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/menu_czynnosci"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/przyciskStronaWWW"
        android:layout_width="178dp"
        android:layout_height="54dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="172dp"
        android:onClick="otworzStrone"
        android:text="@string/otworz_stron_www"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/przyciskZadzwon"
        android:layout_width="166dp"
        android:layout_height="56dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="32dp"
        android:onClick="zadzwon"
        android:text="@string/zadzwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/przyciskZdjecie"
        android:layout_width="170dp"
        android:layout_height="55dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="4dp"
        android:text="@string/zrob_zdjecie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/przyciskZadzwon" />

    <Button
        android:id="@+id/przyciskPowrot1"
        android:layout_width="175dp"
        android:layout_height="57dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="8dp"
        android:onClick="powrot"
        android:text="@string/wroc_do_glownej"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/przyciskStronaWWW" />
</androidx.constraintlayout.widget.ConstraintLayout>